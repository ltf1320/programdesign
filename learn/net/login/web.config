<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
    <pages enableSessionState="true"/>
    <sessionState cookieless="true" timeout="10" mode="InProc"/>
  </system.web>
  <defaultDocument>
    <files>
      <!--默认页-->
      <add value="login.aspx" />
    </files>
  </defaultDocument>
</configuration>